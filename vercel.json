{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "nodejs20.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
